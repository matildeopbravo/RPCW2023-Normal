extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Contrato : #{c._id}

    .w3-container
        ul.w3-ul.w3-card-4
            li #[b NomeInstituição:] #{c.NomeInstituicao}
            li #[b NIPCInstituicao:] #{c.NIPCInstituicao}
            li #[b NomeTitularContrato:] #{c.NomeTitularContrato}
            li #[b CienciaID:] #{c.CienciaID}
            li #[b ORCID:] #{c.ORCID}
            li #[b CienciaVitae:] #{c.CienciaVitae}
            li #[b Carreira_RPN:] #{c.Carreira_RPN}
            li #[b Vinculo_RPN:] #{c.Vinculo_RPN}
            li #[b RegimePrestacaoServico:] #{c.RegimePrestacaoServico}
            li #[b DataInicioContrato:] #{c.DataInicioContrato}
            li #[b DataFimContrato:] #{c.DataFimContrato}
            li #[b ETIremunerado:] #{c.ETIremunerado}
            li #[b ProcedimentoVinculacao:] #{c.ProcedimentoVinculacao}
            li #[b AreasInvestigacao:] #{c.AreasInvestigacao}
            li #[b NivelFormacao:] #{c.NivelFormação}
            li #[b InstituicaoEnsino:] #{c.InstituicaoEnsino}
            li #[b PaisInstituição:] #{c.PaisInstituição}
            li #[b AnoDiploma:] #{c.AnoDiploma}
            li #[b Curso:] #{c.Curso}
            li #[b AreaCNAEF:] #{c.AreaCNAEF}
            li #[b AreaFORD:] #{c.AreaFORD}
            li #[b ProvasAgregacao:] #{c.ProvasAgregacao}
            li #[b TituloEspecialista:] #{c.TituloEspecialista}
            li #[b ProvasCoordenacao:] #{c.ProvasCoordenacao}
            li #[b ProvasAptidao:] #{c.ProvasAptidao}

    footer.w3-container.w3-teal
      h5 Generated by RPCW2023 in #{d} - #[a(href="/") Return]




